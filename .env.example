#To run in development mode:
#flutter run --dart-define=ENV=dev

#To build in development mode:
#flutter build apk --dart-define=ENV=dev

#To run in production mode:
#flutter run --dart-define=ENV=prod

#To build in production mode:
#flutter build apk --dart-define=ENV=prod
# Create these .env files in your project root:

# .env.example (safe to commit to git)
# Copy this file to .env.dev, .env.prod, etc. and fill in actual values

ENV=dev

# Development Environment
DEV_CLIENT_ID=your_dev_client_id_here
DEV_CLIENT_SECRET=your_dev_client_secret_here

# UAT Environment  
UAT_CLIENT_ID=your_uat_client_id_here
UAT_CLIENT_SECRET=your_uat_client_secret_here

# Production Environment
PRD_CLIENT_ID=your_prod_client_id_here
PRD_CLIENT_SECRET=your_prod_client_secret_here